{
    "extends": "@antfu",
    "rules": {
        "no-console": "off",
        "@ts-expect-error": "off",
        "@typescript-eslint/ban-ts-comment": "off"
    },
    "ignorePatterns": [
        "node_modules",
        "dist",
        "public",
        "assets",
        "**.yaml",
        "turbo.json",
        "renovate.json",
        "**content",
        "**locales",
        "**.vscode",
        "output",
        "*.css",
        "*.png",
        "*.ico",
        "*.toml",
        "*.patch",
        "*.txt",
        "Dockerfile",
        "public/"
    ]
}